        "webpack": "^4.41.5",
        "webpack-cli": "^3.3.10",
        "webpack-dev-server": "^3.10.2"
    },
    "dependencies": {
        "chardet": "^0.8.0",
        "chroma-js": "^2.1.0",
        "classnames": "^2.2.6",
        "command-line-args": "^5.1.1",
        "howler": "^2.1.3",
        "iconv-lite": "^0.5.1",
        "jquery": "^3.4.1",
        "lodash": "^4.17.15",
        "music-metadata": "^6.3.3",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-transition-group": "^4.3.0",
        "sharp": "^0.23.4",
        "styled-components": "^5.0.1"
    },
    "build": {
        "appId": "wayne.music",
        "productName": "Wayne Music",
        "icon": "src/resources/img/AppIcon.ico",
        "files": [
            "dist/*",
            "dist/**/*",
            "www/*",
            "www/**/*"
        ],
        "directories": {
            "output": "build"
        },
        "win": {
            "target": "dir"
        }
    }
